{
  "manifest_version": 2,
  "name": "URL to QR code",
  "version": "0.3.0.3",

  "description": "This extension generate QR-code with URL of current openned page.",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "38": "icons/icon38.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },

  "browser_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_title": "Generate QR code",
    "default_popup": "popup.html"
  },

  "options_page": "options.html",

  "permissions": [
    "https://chart.googleapis.com/",
    "tabs",
    "storage"
  ],

  "short_name": "URL 2 QR",

  "author": "Alexey Michurin <a.michurin@gmail.com>",
  "homepage_url": "http://michurin.github.io/"
}
